{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": "off",
        "import/prefer-default-export": "off",
        "import/no-default-export": "error",
        "jsx-quotes": "off",
        "react/prop-types": ["off"],
        "no-unused-vars": "off",
        "react/jsx-curly-newline": "off",
        "object-curly-newline": "off",
        "camelcase":"off",
        "operator-linebreak": "off"
    
    }
}
